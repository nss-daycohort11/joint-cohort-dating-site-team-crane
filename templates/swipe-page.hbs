{{#each this}}
	<div id="swipe-view">
	  	<div id="main-content" class="row">
	  		<div class="col-md-3"></div>
	  		<div class="col-md-6 swipe-view-user-layout">
	  			<div class="row">
		  			<h2 class>{{username}}</h2>
		  		</div>
		  		<div class="row" id="large-user-image">
		  			<img src={{imgurl}}>
		  		</div>
		  		<div class="row" id="userdata">
		  			<p>{{species}} - {{age}} - {{gender}}</p>
		  			<p class="bio">{{bio}}</p>
		  		</div>
		  		<div class="col-md-3"></div>
	  		</div>
	  	</div>
	</div>
{{/each}}

